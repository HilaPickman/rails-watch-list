<div class="container pt-4">
  <h1>List</h1>
  <ul class="list-unstyled py-2">
    <% @lists.each do |list| %>
      <li class="pb-1">
        <%= link_to list.name, list_path(list), :class => "text-decoration-none"%>
      </li>
    <% end %>
  </ul>
   <%= link_to '<i class="fa-regular fa-plus"></i> Add a List'.html_safe, new_list_path, :class => "text-decoration-none" %>
</div>
